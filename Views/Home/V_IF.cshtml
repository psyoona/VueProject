<div>
	<span>V-IF test 입니다.</span>

	<div id="watchTest">
		input1: <input type="text" v-model="a1" /> <br />
		input2: <input type="text" v-model="a2" /> <br />
		<h3 v-if="a1==100">100 {{a1}}</h3>
		<h3 v-else-if="a1==200">200 {{a1}}</h3>

		<h3 v-if="a1==100">re 100 {{a1}}</h3>
		<h3 v-else-if="a1==200">re 200 {{a1}}</h3>

		@* v-if는 렌더링하지 않고, v-show는 가리기만 한다. *@

		<h3>{{a2}}</h3>
		<h3>{{a3}}</h3>
	</div>
</div>

<script defer type="text/javascript" src="~/js/v-if.js"></script>